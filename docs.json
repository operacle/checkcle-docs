{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "CheckCle",
  "colors": {
    "primary": "#1EBCA1",
    "light": "#1EBCA1",
    "dark": "#1EBCA1"
  },
  "favicon": "/favicon.svg",
  "contextual": {
    "options": ["copy", "view", "chatgpt", "claude"]
  },
  "navigation": {
    "global": {
      "anchors": [
        {
          "anchor": "CheckCle Homepage",
          "icon": "house",
          "href": "https://checkcle.io"
        },
        {
          "anchor": "Blog & Tutorials",
          "icon": "newspaper",
          "href": "https://checkcle.io/blog"
        },
        {
          "anchor": "Discord",
          "icon": "discord",
          "href": "https://discord.gg/xs9gbubGwX"
        },
        {
          "anchor": "GitHub",
          "icon": "github",
          "href": "https://github.com/operacle/checkcle"
        }
      ]
    },
    "tabs": [
      {
        "tab": "Documentation",
        "groups": [
          {
            "group": "Getting Started",
            "pages": [
              "introduction",
              "quickstart",
              "local-development",
              "libsql"
            ]
          },
          {
            "group": "Product",
            "pages": [
              "features/platform-api",
              "features/ai-and-embeddings",
              "features/embedded-replicas/introduction",
              "features/branching",
              "features/point-in-time-recovery",
              "features/scale-to-zero",
              "features/sqlite-extensions"
            ]
          },
          {
            "group": "Cloud",
            "pages": [
              "cloud/migrate-to-checkcle",
              "cloud/durability",
              "cloud/private-endpoints"
            ]
          },
          {
            "group": "Help",
            "pages": [
              "support",
              "help/usage-and-billing",
              "data-and-connections"
            ]
          }
        ]
      },
      {
        "tab": "SDKs",
        "groups": [
          {
            "group": "Official SDKs",
            "pages": [
              "sdk/ts/quickstart",
              "sdk/ts/reference",
              "sdk/go/quickstart",
              "sdk/go/reference",
              "sdk/python/quickstart",
              "sdk/python/reference"
            ]
          }
        ]
      },
      {
        "tab": "checkcle CLI",
        "groups": [
          {
            "group": "checkcle CLI",
            "pages": [
              "cli/introduction",
              "cli/installation",
              "cli/authentication",
              "cli/upgrading",
              "cli/help"
            ]
          }
        ]
      },
      {
        "tab": "Platform API",
        "groups": [
          {
            "group": "API",
            "pages": [
              "api-reference/introduction",
              "api-reference/quickstart",
              "api-reference/authentication",
              "api-reference/response-codes"
            ]
          }
        ]
      }
    ]
  },
  "logo": {
    "light": "/logo/checkcle-light.svg",
    "dark": "/logo/checkcle-dark.svg"
  },
  "api": {
    "mdx": {
      "server": "https://api.CheckCle",
      "auth": {
        "method": "bearer"
      }
    }
  },
  "appearance": {
    "default": "dark"
  },
  "background": {
    "color": {
      "light": "#FFFFFF",
      "dark": "#121B22"
    }
  },
  "navbar": {
    "links": [
      {
        "label": "API Status",
        "href": "https://status.CheckCle"
      },
      {
        "label": "Dashboard",
        "href": "https://CheckCle/app"
      }
    ],
    "primary": {
      "type": "github",
      "href": "https://github.com/checkcledatabase/checkcle"
    }
  },
  "footer": {
    "socials": {
      "discord": "https://discord.gg/checkcle",
      "github": "https://github.com/checkcledatabase",
      "twitter": "https://twitter.com/checkcledatabase",
      "linkedin": "https://www.linkedin.com/company/checkcle"
    }
  },
  "integrations": {
    "gtm": {
      "tagId": "GTM-WW757Q5"
    }
  },
  "redirects": [
    {
      "source": "/concepts",
      "destination": "/introduction"
    },
    {
      "source": "/tutorials",
      "destination": "https://CheckCle/blog"
    },
    {
      "source": "/tutorials/get-started-checkcle-cli",
      "destination": "/quickstart"
    },
    {
      "source": "/reference",
      "destination": "/cli"
    },
    {
      "source": "/reference/extensions",
      "destination": "/features/sqlite-extensions"
    },
    {
      "source": "/reference/platform-rest-api",
      "destination": "/api-reference/introduction"
    },
    {
      "source": "/locations",
      "destination": "/introduction"
    },
    {
      "source": "/cloud-providers",
      "destination": "/introduction"
    }
  ]
}
